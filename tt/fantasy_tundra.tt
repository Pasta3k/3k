#alias {hk} {tkeep;#2 w}
#alias {fhk} {#2 e;ftkeep}

#nop 130 = Catacombs by Nanook.
#alias {cata} {fr;#7 e;n;d}
#alias {fcata} {u;s;#7 w;fanpri}

#nop 131 = Mage Tower by Rastafan.
#alias {hcmage} {fr;#12 e;n;enter}
#alias {fhcmage} {leave;s;#12 w;fanpri}

#nop 132 = Roc's Nest.
#alias {roc} {fr;#14 e;n;climb}
#alias {froc} {leave;s;#14 w;fanpri}

#nop 152 = Troll Den by Peeron.
#alias {superheal} {fi;#5 s;#4 e;enter}
#alias {fsuperheal} {leave;#4 w;#5 n;fanpri}

